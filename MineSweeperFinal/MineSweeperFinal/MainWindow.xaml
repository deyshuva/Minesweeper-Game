<Window x:Name="start" x:Class="MineSweeperFinal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MineSweeperFinal"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
    <Grid Height="430" Width="480 " Margin="3,-15,0,-11" Background="#FFCFCFCF">
        <Button x:Name="set_button" Content="Set Field" HorizontalAlignment="Left" VerticalAlignment="Top"
                Height="40" Width="100" Background="#FFF7F7F7" FontSize="16" Margin="76,103,0,0" Click="set_button_Click"/>
        
        <Button x:Name="play_button" Content="Play Game" HorizontalAlignment="Left" Margin="76,189,0,0" VerticalAlignment="Top"
                Height="40" Width="100" Background="#FFF7F7F7" FontSize="16" Click="play_button_Click"/>
        

    </Grid>
</Window>
